/**
 * An abstract type to describe a query string object.
 */
type queryString;

/**
 * An abstract type to describe an Apollo Link object.
 */
type apolloLink;

/**
 * An abstract type to describe an Apollo Cache object.
 */
type apolloCache;

/**
 * An abstract type to describe fetch.
 */
type fetch;

/**
 * An abstract type to describe a storage type supported by apollo-cache-persist.
 */
type storage;
